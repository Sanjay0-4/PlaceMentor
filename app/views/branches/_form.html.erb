<%= render 'shared/errors', obj: @branch %>

<div class="row">
    <div class = "col-md-6 col-md-offset-3 well" style="background-color: rgba(245, 245, 245, 0.3);">
        <strong><h3 class="text-center">Create New Branch</h3></strong>
      
           <br><br>
        <%= form_for @branch do |f| %>
         <%= f.label :name %>  <%= f.text_field :name %>
         
        <%= f.submit 'Create Branch',class: "btn btn-warning" %>
        <% end %>
    </div>
</div>

